{
  "title": "My Settings",
  "rules":
  [
	  {
		  "description": "Ctrl + j --> Return",
		  "manipulators":[
			  {
			  "type": "basic",
			  "from":{
				  "key_code": "j",
				  "modifiers":{"mandatory":["control"]}
			  },
			  "to":[{"key_code": "return_or_enter"}]
			  }
		  ]
	  },
	  {
		  "description": "Left Option + h --> Left",
		  "manipulators":[
			  {
			  "type": "basic",
			  "from":{
				  "key_code": "h",
				  "modifiers":{"mandatory":["left_option"]}
			  },
			  "to":[{"key_code": "left_arrow"}]
			  }
		  ]
	  },
	  {
		  "description": "Left Option + j --> Down",
		  "manipulators":[
			  {
			  "type": "basic",
			  "from":{
				  "key_code": "j",
				  "modifiers":{"mandatory":["left_option"]}
			  },
			  "to":[{"key_code": "down_arrow"}]
			  }
		  ]
	  },
	  {
		  "description": "Left Option + k --> Up",
		  "manipulators":[
			  {
			  "type": "basic",
			  "from":{
				  "key_code": "k",
				  "modifiers":{"mandatory":["left_option"]}
			  },
			  "to":[{"key_code": "up_arrow"}]
			  }
		  ]
	  },
	  {
		  "description": "Left Option + l --> Right",
		  "manipulators":[
			  {
			  "type": "basic",
			  "from":{
				  "key_code": "l",
				  "modifiers":{"mandatory":["left_option"]}
			  },
			  "to":[{"key_code": "right_arrow"}]
			  }
		  ]
	  }
  ]
}
